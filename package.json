{
  "name": "mmpilot-basic-template",
  "version": "1.0.0",
  "description": "This is a basic starter template for MMPilot.",
  "scripts": {
    "clean": "mmpilot clean",
    "build": "mmpilot build",
    "serve": "mmpilot serve",
    "dev-build": "mmpilot build -d",
    "dev-watch": "mmpilot watch -d",
    "dev": "npm-run-all clean dev-build --parallel dev-watch serve",
    "prod": "npm-run-all clean build serve",
    "deploy": "npm-run-all clean build && mmpilot deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunruch/mmpilot-basic-template.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kunruch/mmpilot-basic-template/issues"
  },
  "homepage": "https://github.com/kunruch/mmpilot-basic-template#readme",
   "devDependencies": {
    "mmpilot": "^0.6.3",
    "npm-run-all": "^4.0.1"
  },
  "dependencies": {}
}
